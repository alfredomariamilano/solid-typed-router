{
  "name": "@bizarreal/vite-plugin-solid-typed-router",
  "version": "0.0.2",
  "description": "A vite plugin to create typed routes for Solid projects, including Solid Start.",
  "type": "module",
  "author": {
    "name": "Alfredo Maria Milano",
    "email": "hi@alfredomm.com",
    "url": "https://www.alfredomm.com/"
  },
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w"
  },
  "source": "src/index.ts",
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "typings": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    },
    "./package.json": "./package.json"
  },
  "license": "MIT",
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@types/node": "^22.8.1",
    "rollup-plugin-dts": "^6.1.1",
    "vite": "^5.4.10"
  },
  "peerDependencies": {
    "@solidjs/router": ">= 0.14.0 < 1",
    "lodash-es": ">= 4.0.0",
    "rollup": ">= 4.0.0",
    "rollup-plugin-esbuild": ">= 6.0.0",
    "solid-js": ">= 1.9.0 < 2",
    "valibot": "^1.0.0-beta.3"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
